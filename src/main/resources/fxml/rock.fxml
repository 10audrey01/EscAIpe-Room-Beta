            <ImageView fitHeight="339.0" fitWidth="89.0" layoutX="-3.0" preserveRatio="true">
               <image>
                  <Image url="@../images/rockRoom/guitar1.png" />
               </image>
            </ImageView>
         </children></Pane>
      <Pane fx:id="blueGuitarPane" layoutX="264.0" layoutY="16.0" onMouseClicked="#onClickBlueGuitar" prefHeight="296.0" prefWidth="76.0">
         <children>
            <ImageView fitHeight="324.0" fitWidth="82.0" layoutX="-5.0" layoutY="-3.0" preserveRatio="true">
               <image>
                  <Image url="@../images/rockRoom/guitar2.png" />
               </image>
            </ImageView>
         </children></Pane>
      <Pane fx:id="yellowGuitarPane" layoutX="346.0" layoutY="66.0" onMouseClicked="#onClickYellowGuitar" prefHeight="316.0" prefWidth="82.0">
         <children>
            <ImageView fitHeight="388.0" fitWidth="82.0" layoutX="-2.0" layoutY="-4.0" preserveRatio="true">
               <image>
                  <Image url="@../images/rockRoom/guitar3.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane fx:id="gameMasterPane" layoutX="491.0" layoutY="171.0" onMouseClicked="#onClickGameMaster" prefHeight="154.0" prefWidth="148.0" />
