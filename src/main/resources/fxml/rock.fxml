            <Font size="30.0" />
         </font>
      </Label>
      <Pane fx:id="chatBoxPane" disable="true" layoutX="25.0" layoutY="510.0" opacity="0.0" prefHeight="195.0" prefWidth="1232.0" style="-fx-background-color: white; -fx-background-radius: 5px;">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="195.0" prefWidth="1232.0">
               <children>
                  <TextArea fx:id="textArea" editable="false" prefHeight="159.0" prefWidth="1222.0">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TextArea>
                  <TextField fx:id="textField" promptText="Enter Response Here">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </Pane>
      <Button fx:id="toggleChatButton" layoutX="1150.0" layoutY="465.0" mnemonicParsing="false" onAction="#toggleChat" text="Toggle Chat" />
