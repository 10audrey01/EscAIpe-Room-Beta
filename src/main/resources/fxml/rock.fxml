      <Pane fx:id="colourPane2" layoutX="1032.0" layoutY="37.0" prefHeight="25.0" prefWidth="25.0" />
      <Pane fx:id="colourPane3" layoutX="1063.0" layoutY="37.0" prefHeight="25.0" prefWidth="25.0" />
      <Pane fx:id="colourPane4" layoutX="1094.0" layoutY="37.0" prefHeight="25.0" prefWidth="25.0" />
   </children>
</Pane>
