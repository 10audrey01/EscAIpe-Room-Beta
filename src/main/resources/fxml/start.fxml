      <RadioButton fx:id="twoMinsBtn" layoutX="162.0" layoutY="234.0" mnemonicParsing="false" onMouseClicked="#onClickTwo" text="2 mins" />
      <RadioButton fx:id="fourMinsBtn" layoutX="275.0" layoutY="234.0" mnemonicParsing="false" onMouseClicked="#onClickFour" selected="true" text="4 mins" />
